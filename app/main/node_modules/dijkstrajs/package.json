{
  "name": "dijkstrajs",
  "version": "1.0.2",
  "description": "A simple JavaScript implementation of Dijkstra's single-source shortest-paths algorithm.",
  "main": "dijkstra.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/dijkstrajs"
  },
  "license": "MIT",
  "homepage": "https://github.com/tcort/dijkstrajs",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.12.0",
    "mocha": "^8.4.0"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}